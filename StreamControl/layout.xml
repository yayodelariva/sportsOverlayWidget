<!DOCTYPE StreamControlLayout>
<layout width="400" height="280" tabbed="1">
 <tab name="Match Info">
  <label x="10" y="14" width="46" height="13">Team L</label>
  <lineEdit id="teamLeft" x="60" y="10" width="170" height="20"/>
  <label x="250" y="10" width="46" height="13">Score</label>
  <spinBox id="teamLeftScore" x="290" y="6" width="42" height="22" maximum="999" />
  <label x="10" y="44" width="46" height="13">Team R</label>
  <lineEdit id="teamRight" x="60" y="40" width="170" height="20"/>
  <label x="250" y="40" width="46" height="13">Score</label>
  <spinBox id="teamRightScore" x="290" y="36" width="42" height="22" maximum="999" />
  <label x="70" y="130" width="46" height="13">Round</label>
  <lineEdit id="round" x="120" y="130" width="170" height="20"/>

  <button type="swap" x="10" y="82" width="60" height="23" tooltip="Swap the Scores" id="swap1" swapSet1="teamLeft" swapSet2="teamRight">Swap</button>
  <button type="reset" x="80" y="82" width="60" height="23" tooltip="Clear Match Info" id="reset1" reset="teamLeft,teamRight">Clear</button>
  <button type="swap" x="235" y="82" width="60" height="23" tooltip="Swap the Scores" id="swap2" swapSet1="teamLeftScore" swapSet2="teamRightScore">Swap</button>
  <button type="reset" x="310" y="82" width="60" height="23" tooltip="Clear Match Info" id="reset2" reset="teamLeftScore,teamRightScore">Clear</button>
  <button type="reset" x="140" y="170" width="60" height="23" tooltip="Clear Match Info" id="reset3" reset="round">Clear</button>

 </tab>
</layout>
